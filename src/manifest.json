{
  "name": "Facebook-Lock",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Prototype for locking Facebook.",
  "background": {
    "scripts": ["static/background.js"]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "static/unlock.png",
	"default_popup": "popup.html"
  },
  "permissions": [
	"tabs", "idle", "cookies", "*://*.facebook.com/", "background", "*://*/*"]
}
